<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.redhatchallenge.rhc2013.shared.RegStatus" table="registration_status">
        <id name="status_id" column="status_id">
            <generator class="assigned"/>
        </id>
        <property name="reg_status_bool" column="reg_status_bool" type="boolean"/>
    </class>
</hibernate-mapping>                                                                           